<div class="pres">
  <% @pre.each do |p|%>
  <div style="background-color: white;border-radius: 15px;padding-top: 1%">
  <div class="row" >
     <!--新闻开始迭代-->
                            
      <div class="span12 centered">
          <h2><%= p.title %></h2>
          <p class="text-center"><%= p.created_at.strftime('%Y-%m-%d %H:%M:%S').to_s%></p>
          <div class="breaker"><span class="left"></span><div class="feather"></div><span class="right"></span></div>
      </div>
  </div>
  <div class="row">
      <div class="span12 centered">
          <div style="padding:2%">
            <div style="padding:2%">
              <%=raw p.content %>
            </div>
          </div>
      </div>
    </div>
    </div>
     <div class="row" style="background-color: #F5F5F5;">
      <div class="span12 centered">
          <div style="padding:2%">
            <div style="padding:2%">
              
            </div>
          </div>
      </div>
    </div>
    
  
  <% end %>
  <div class="text-center" onclick="window.location = '#about'">
    <%= paginate @pre, :remote => true, :param_name => :pres_page%>
  </div>
</div>
